<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="view.MainViewController" style="-fx-background-color: #e6e6e6">
	<children>
		<ToolBar translateX="765.0" style="-fx-background-color: #e6e6e6">
			<items>
				<Button styleClass="red" maxHeight="20.0" translateY="-6.0"
					fx:id="closeButton" mnemonicParsing="_F4" />
			</items>
		</ToolBar>
		<Label alignment="CENTER" contentDisplay="CENTER" translateY="6.0"
			translateX="275.0" text="STUDENT MANAGEMENT SYSTEM" textAlignment="CENTER"
			textFill="#00986e" style="-fx-font-weight: Bold;">
			<font>
				<Font size="15.0" />
			</font>
		</Label>

		<TabPane minHeight="570.0" minWidth="800.0" tabClosingPolicy="UNAVAILABLE"
			translateY="30.0">
			<tabs>
				<Tab text="Courses">
					<content>
						<BorderPane>
							<top>
								<Label alignment="CENTER" contentDisplay="CENTER"
									prefHeight="46.0" prefWidth="700.0" text="LIST OF COURSES"
									textAlignment="CENTER" textFill="#6aa6ff" BorderPane.alignment="CENTER">
									<font>
										<Font size="41.0" />
									</font>
								</Label>
							</top>
							<center>
								<TableView BorderPane.alignment="CENTER">
									<columns>
										<TableColumn prefWidth="115.0" text="Course ID" />
										<TableColumn prefWidth="115.0" text="Course Name" />
										<TableColumn prefWidth="115.0" text="Course Start" />
										<TableColumn prefWidth="115.0" text="Course End" />
									</columns>
								</TableView>
							</center>
						</BorderPane>
					</content>
				</Tab>
				<Tab text="Class">
					<content>
						<BorderPane>
							<top>
								<Label alignment="CENTER" contentDisplay="CENTER"
									prefHeight="46.0" prefWidth="700.0" text="LIST OF CLASSES"
									textAlignment="CENTER" textFill="#6aa6ff" BorderPane.alignment="CENTER">
									<font>
										<Font size="41.0" />
									</font>
								</Label>
							</top>
							<center>
								<TableView BorderPane.alignment="CENTER">
									<columns>
										<TableColumn prefWidth="115.0" text="Class ID" />
										<TableColumn prefWidth="115.0" text="Class Name" />
										<TableColumn prefWidth="115.0" text="Class Room" />
									</columns>
								</TableView>
							</center>
						</BorderPane>
					</content>
				</Tab>
				<Tab text="Student">
					<content>
						<BorderPane />
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</children>
</Pane>
